// schema.prisma

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "darwin"]
}



datasource db {
  provider          = "postgresql"
  // url               = env("DATABASE_URL") // Use a common environment variable name
  url               = env("POSTGRES_PRISMA_URL")
  directUrl         = env("POSTGRES_URL_NON_POOLING")
  shadowDatabaseUrl = env("POSTGRES_URL_NON_POOLING")
}


model Graduate {
  id        Int @id @default(autoincrement())
  fullName  String?
  github    String?
  avatar    String?
  linkedin  String?
  role      String? 
  cohort    String?
}

